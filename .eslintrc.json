{
	"env": {
		"browser": true,
		"es2021": true,
		"jest/globals": true
	},
	"extends": ["plugin:react/recommended","plugin:jest/recommended" ,"airbnb", "prettier"],

	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react","jest"],
	"rules": {
	},
	"import/no-extraneous-dependencies": ["error", {"devDependencies": false, "optionalDependencies": false, "peerDependencies": false}]
}
